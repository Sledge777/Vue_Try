sts","searchedAndSortLists"],"sources":["C:\\Users\\bogda\\Downloads\\Vue_Try\\src\\pages\\ListsWithVuex.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <my-input\r\n        :model-value=\"search\"\r\n        @update.model-value=\"setSearch\"\r\n        placeholder=\"Поиск\"\r\n        v-focus\r\n        >\r\n        </my-input>\r\n\r\n        <div class=\"buttons\">\r\n            <first-Button\r\n            @click=\"showDialog\"\r\n            style=\"margin-top: 10px;\"\r\n            >Создать пост</first-Button>\r\n            <my-select \r\n            :modelValue=\"selectedSort\"\r\n            @update.modelValue=\"setSort\"\r\n            :options=\"sortOptions\"\r\n            >\r\n\r\n            </my-select>\r\n        </div>\r\n        <my-dialog v-model:show=\"dialogVisible\">\r\n            <post-form \r\n                @getPost=\"add\"\r\n            />\r\n        </my-dialog>\r\n        <post-list \r\n        :lists=\"searchedAndSortLists\" \r\n        @remove=\"remove\"\r\n        />\r\n        <div class=\"wrapper_page\">\r\n            <div \r\n            class=\"page\" \r\n            v-for=\"pageNum in maxLists\" \r\n            :key=\"pageNum\"\r\n            :class=\"{\r\n                'selected_page': this.page === pageNum\r\n            }\"\r\n            @click=\"changePage(pageNum)\"\r\n            >{{ pageNum }}</div>\r\n        </div>\r\n        <div v-intersection=\"fetchLists\" class=\"observe\"></div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport { mapMutations,mapActions,mapGetters,mapState } from 'vuex';\r\nimport PostList from \"@/components/PostList\";\r\nimport PostForm from \"@/components/PostForm\";\r\n\r\nexport default {\r\n    components: {\r\n        PostList, PostForm,\r\n    },\r\n    data() {\r\n        return {\r\n            dialogVisible: false,\r\n        }\r\n    },   \r\n    methods: {\r\n        ...mapActions ({\r\n            fetchLists: 'ListModule/fetchLists',\r\n            changePage: 'ListModule/changePage',\r\n        }),\r\n        ...mapMutations ({\r\n            setPage: 'ListModule/setPage',\r\n            setSearch: 'ListModule/setSearch',\r\n            setSort: 'ListModule/setSort'\r\n        }),\r\n        createPost(post) {\r\n            this.posts.push(post);\r\n            this.dialogVisible = false;\r\n        },\r\n        removePost(post) {\r\n            this.posts = this.posts.filter(p => p.id !== post.id)\r\n        },\r\n        showDialog() {\r\n            this.dialogVisible = true;\r\n        },\r\n    },\r\n    mounted() {\r\n        this.fetchLists()\r\n    },\r\n    computed: { \r\n        ...mapState ({\r\n            lists: state => state.ListModule.lists,\r\n            selectedSort: state => state.ListModule.selectedSort,\r\n            page: state => state.ListModule.page,\r\n            limit: state => state.ListModule.limit,\r\n            maxLists: state => state.ListModule.maxLists,\r\n            sortOptions: state => state.ListModule.sortOptions,\r\n            search: state => state.ListModule.search,\r\n        }),\r\n        ...mapGetters ({\r\n            sortedLists: 'ListModule/sortedLists',\r\n            searchedAndSortLists: 'ListModule/searchedAndSortLists'\r\n        }),\r\n        \r\n    },\r\n    // watch: {\r\n    //     page() {\r\n    //         this.fetchLists\r\n    //     } \r\n    // }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n    padding: 20px;\r\n}\r\n.buttons {\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n.wrapper_page {\r\n    display: flex;\r\n    margin-top: 20px;\r\n}\r\n.page {\r\n    border:1px solid black ;\r\n    padding: 10px;\r\n}\r\n.selected_page {\r\n    border:2px solid green;\r\n}\r\n.observe {\r\n    height: 30px;\r\n}\r\n</style>"],"mappings":";AA+CA,SAASA,YAAY,EAACC,UAAU,EAACC,UAAU,EAACC,QAAO,QAAS,MAAM;AAClE,OAAOC,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,QAAO,MAAO,uBAAuB;AAE5C,eAAe;EACXC,UAAU,EAAE;IACRF,QAAQ;IAAEC;EACd,CAAC;EACDE,IAAIA,CAAA,EAAG;I